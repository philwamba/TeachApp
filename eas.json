{
  "cli": {
    "version": ">= 10.1.1"
  },
  "build": {
    "release": {
      "android": {
        "buildType": "apk"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk",
        "distribution": "store",
        "releaseChannel": "default",
        "developmentClient": false,
        "env": {
          "EXPO_USE_HERMES": "true"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
